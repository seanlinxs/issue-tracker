{
  "name": "issue-tracker",
  "version": "1.0.0",
  "description": "Issue Tracker focus on simplicity",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.bundle.js",
    "compile-server": "webpack --config webpack.server-config.js",
    "watch-server": "webpack --config webpack.server-config.js --watch",
    "start-hook": "nodemon -e js,jsx -w server,src server/start_hook.js",
    "compile": "webpack",
    "watch": "webpack-dev-server --hot --inline",
    "dev-all-hook": "npm run watch & npm run start-hook",
    "dev-all": "npm run watch & npm run watch-server & npm run start",
    "lint": "eslint --ext js,jsx client src server webpack.config.js, webpack.server-config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanlinxs/issue-tracker.git"
  },
  "keywords": [
    "Issue",
    "Tracker",
    "Project",
    "Issue",
    "Management"
  ],
  "author": "Sean Lin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanlinxs/issue-tracker/issues"
  },
  "homepage": "https://github.com/seanlinxs/issue-tracker#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "express": "^4.15.2",
    "mongodb": "^2.2.26",
    "object-assign": "^4.1.1",
    "prop-types": "^15.5.8",
    "query-string": "^4.3.4",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.1",
    "source-map-support": "^0.4.15",
    "webpack": "^2.4.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "nodemon": "^1.11.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  }
}
